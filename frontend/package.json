{
  "name": "frontend",
  "private": true,
  "browserslist": [
    "defaults",
    "not ie 11",
    "maintained node versions"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "prisma:update": "prisma db pull && prisma generate",
    "playwright:install": "playwright install",
    "playwright:install:withDebs": "playwright install --with-deps",
    "test:e2e": "playwright test",
    "test:unit:watch": "vitest watch --ui",
    "test:unit:ci": "vitest run --coverage",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "lint:fix": "prettier --write --plugin-search-dir=. . && eslint --fix ."
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "checkeasy": "^1.1.0",
    "nprogress": "^0.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.25.1",
    "@sveltejs/adapter-node": "^1.0.0-next.87",
    "@sveltejs/kit": "^1.0.0-next.445",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.35.1",
    "@typescript-eslint/parser": "^5.35.1",
    "@vitest/ui": "^0.22.1",
    "autoprefixer": "^10.4.8",
    "browserslist": "^4.21.3",
    "discord-api-types": "^0.37.5",
    "eslint": "8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "open-props": "^1.4.14",
    "postcss": "^8.4.16",
    "postcss-jit-props": "^1.0.7",
    "postcss-preset-env": "^7.8.0",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "prisma": "^4.2.1",
    "sass": "^1.54.5",
    "sk-auth": "^0.4.1",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.1",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.8.2",
    "unplugin-icons": "^0.14.8",
    "vite": "^3.0.9",
    "vitest": "^0.22.1"
  },
  "type": "module"
}