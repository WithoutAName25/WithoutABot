{
  "name": "frontend",
  "private": true,
  "browserslist": [
    "defaults",
    "not ie 11",
    "maintained node versions"
  ],
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prisma:pull": "prisma db pull",
    "prisma:generate": "prisma generate",
    "prisma:update": "prisma db pull && prisma generate",
    "playwright:install": "playwright install",
    "playwright:install:withDebs": "playwright install --with-deps",
    "test:e2e": "playwright test",
    "test:unit:watch": "vitest watch --ui",
    "test:unit:ci": "vitest run --coverage",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check --plugin-search-dir=. . && eslint .",
    "lint:fix": "prettier --write --plugin-search-dir=. . && eslint --fix .",
    "upgrade": "npm-check-updates -u"
  },
  "dependencies": {
    "@prisma/client": "^4.1.1",
    "checkeasy": "^1.1.0",
    "nprogress": "^0.2.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.24.0",
    "@sveltejs/adapter-node": "1.0.0-next.83",
    "@sveltejs/kit": "1.0.0-next.401",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^5.30.7",
    "@typescript-eslint/parser": "^5.30.7",
    "@vitest/ui": "^0.22.1",
    "autoprefixer": "^10.4.7",
    "browserslist": "^4.21.2",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-svelte3": "^4.0.0",
    "open-props": "^1.4.4",
    "postcss": "^8.4.14",
    "postcss-jit-props": "^1.0.7",
    "postcss-preset-env": "^7.7.2",
    "prettier": "^2.7.1",
    "prettier-plugin-svelte": "^2.7.0",
    "prisma": "^4.1.1",
    "sass": "^1.54.0",
    "sk-auth": "^0.4.1",
    "svelte": "^3.49.0",
    "svelte-check": "^2.8.0",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.7.4",
    "unplugin-icons": "^0.14.7",
    "vite": "^3.0.3",
    "vitest": "^0.19.1"
  },
  "type": "module"
}
